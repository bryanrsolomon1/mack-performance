{
    "development": {
      "client": {
        "API_SERVER": "localhost",
        "ENVIRONMENT": "beta",
        "LOGIN_TIMEOUT_MIN": 5,
        "LOGOUT_WARNING_SECONDS": 30
      },
      "gulp": {
        "env": "dev",
        "dist": "./dist.dev/",
        "distApp": "./dist.dev/app/",
        "distBower": "./dist.dev/bower_components/"
      }
    },
    "production": {
      "client": {
        "API_SERVER": "localhost",
        "ENVIRONMENT": "prod",
        "LOGIN_TIMEOUT_MIN": 5,
        "LOGOUT_WARNING_SECONDS": 30
      },
      "gulp": {
        "env": "pro",
        "dist": "./dist.pro/",
        "distApp": "./dist.pro/app/",
        "distBower": "./dist.pro/bower_components/"
      }
    }
}